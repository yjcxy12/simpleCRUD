{
  "name": "simple_crud",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "lint": "node_modules/eslint/bin/eslint.js .",
    "build:dev": "webpack client/index.js --config webpack.config.develop.js --progress --colors",
    "build:min": "webpack client/index.js --config webpack.config.production.js --progress --colors",
    "build:server": "babel ./server --out-dir ./dist && cp ./server/data.json ./dist/data.json",
    "serve:client": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000 --config webpack.config.devserver.js",
    "serve": "node dist/index.js",
    "prestart:dev": "npm run clean && npm run build:dev && npm run build:server",
    "start:dev": "npm run serve", 
    "prestart": "npm run clean && npm run build:min && npm run build:server",
    "start": "npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yjcxy12/simpleCRUD.git"
  },
  "author": "Xiangyun Chi <xiangyunchi@sis.tv>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yjcxy12/simpleCRUD/issues"
  },
  "homepage": "https://github.com/yjcxy12/simpleCRUD#readme",
  "dependencies": {
    "body-parser": "^1.15.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "material-ui": "^0.15.0-alpha.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.8.0-beta.2"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.9.0",
    "node-sass": "^3.4.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
